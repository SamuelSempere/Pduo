{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "app/api/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "app/api/(.*)",
        "dest": "http://localhost:1337/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "dev": {
      "env": {
        "NODE_ENV": "development"
      }
    }
  }